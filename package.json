{
  "name": "ui-harness",
  "version": "3.0.3-beta",
  "description": "Isolate, test and document modular UI with React.",
  "scripts": {
    "start": "node start --entry=./src/specs",
    "postinstall": "npm run bundle:init",
    "bundle:init": "node ./bundle --init",
    "test": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-register",
    "tdd": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-register --watch",
    "prepublish": "gulp lint && gulp build"
  },
  "dependencies": {
