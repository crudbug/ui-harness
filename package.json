    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "bluebird": "^2.9.33",
    "chalk": "^1.1.1",
    "color": "^0.10.1",
    "compression": "^1.5.2",
    "express": "^4.13.0",
    "file-loader": "^0.8.4",
    "filesize": "^3.1.5",
    "fs-extra": "^0.24.0",
    "immutable": "^3.7.4",
    "js-bdd": "^2.0.0",
    "js-util": "^2.0.2",
    "json-loader": "^0.5.2",
    "marked": "^0.3.5",
    "minimist": "^1.2.0",
    "moment": "^2.11.1",
    "node-libs-browser": "^0.5.2",
    "radium": "^0.13.2",
    "ramda": "^0.17.1",
    "react": "^0.14.6",
    "react-atoms": "^2.0.6",
    "react-dom": "^0.14.6",
    "react-object": "^2.0.9",
    "react-schema": "^1.1.0",
    "shelljs": "^0.5.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "^1.1.1",
    "gulp-plumber": "^1.0.1"
