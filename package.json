{
  "name": "ui-harness",
  "version": "3.1.0",
  "description": "Isolate, test and document modular UI with React.",
  "scripts": {
    "start": "node start --entry=./src/specs",
    "postinstall": "npm run bundle:init",
    "bundle:init": "node ./bundle --init",
    "bundle:init-prod": "npm run bundle:init -- --prod",
    "test": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-register",
    "tdd": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-register --watch",
    "lint": "eslint ./src",
    "build": "babel src --out-dir lib --source-maps",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm test && npm run lint && npm build"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "babel-loader": "^6.2.0",
    "babel-preset-react": "^6.3.13",
    "bluebird": "^3.1.4",
    "chalk": "^1.1.1",
    "color": "^0.11.1",
    "compression": "^1.5.2",
    "express": "^4.13.0",
    "express-graphql": "^0.4.5",
    "file-loader": "^0.8.4",
    "filesize": "^3.1.5",
    "fs-extra": "^0.26.4",
    "graphql": "^0.4.14",
    "graphql-relay": "^0.3.6",
    "immutable": "^3.7.4",
    "js-babel": "^6.0.0",
    "js-bdd": "^2.0.0",
    "js-util": "^2.0.2",
    "json-loader": "^0.5.2",
    "marked": "^0.3.5",
    "minimist": "^1.2.0",
    "moment": "^2.11.1",
    "node-libs-browser": "^1.0.0",
    "radium": "^0.16.4",
    "ramda": "^0.19.1",
    "react": "^0.14.6",
    "react-atoms": "^2.0.6",
    "react-dom": "^0.14.6",
    "react-object": "^2.0.9",
    "react-relay": "^0.6.1",
    "react-schema": "^1.1.0",
    "semver": "^5.1.0",
    "shelljs": "^0.5.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-relay-plugin": "^0.6.3",
    "chai": "^3.4.1",
    "js-babel-dev": "^6.0.0",
    "mocha": "^2.2.1",
    "sinon": "^1.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/ui-harness.git"
  },
  "keywords": [
    "ui",
    "testing",
    "react",
    "modular",
    "design",
    "react-component"
  ],
  "author": "Phil Cockfield",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philcockfield/ui-harness/issues"
  },
  "homepage": "http://uiharness.com"
}
